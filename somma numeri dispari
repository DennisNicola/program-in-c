#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
		
		int i = 0;
		int n = 10;
		int somma = 0;
		while(i<=n){
					if (i%2==1){
								somma = somma + i;
								
								}
					i = i + 1;
					}
		printf("la somma dei numeri dispari Ã¨: %d", somma);
		return 0;
}
